O(n*log(m)) approach with O(1)space
void Solution::merge(vector<int> &a, vector<int> &b) {
    int n=a.size(),m=b.size();
    for(int i=0;i<n;i++){
        if(a[i]>b[0]){
            swap(a[i],b[0]);
            if(m!=1){
                 int  x= upper_bound(b.begin()+1,b.end(),b[0])-b.begin();
                 x--;
                 swap(b[0],b[x]);
            }
        }
    }
    for(int i=0;i<m;i++){
        a.push_back(b[i]);
    }
}
