void calc(vector<int> &A,vector<vector<int>> &ans,vector<int> res,int index)
{
    ans.push_back(res);
    for(int i=index;i<A.size();i++)
    {
        res.push_back(A[i]);
        calc(A,ans,res,i+1);
        res.pop_back();
    }
    return;
}
vector<vector<int> > Solution::subsets(vector<int> &A) {
    vector<vector<int>> ans;
    sort(A.begin(),A.end());
    int index=0;
    vector<int> res;
    calc(A,ans,res,index);
    return ans;
    
}
