void calc(vector<int>&A, int l, int r,  vector<vector<int>>&ans){
    if(l==r){
        ans.push_back(A);
    }
    else{
        for(int i=l;i<=r;i++){
            swap(A[l],A[i]);
            calc(A,l+1,r,ans);
            swap(A[l],A[i]);
        }
    }
}
vector<vector<int> > Solution::permute(vector<int> &A) {
    vector<vector<int>>ans;
    int n=A.size();
    calc(A,0,n-1,ans);
    return ans;
}
