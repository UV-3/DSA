const int N=2e5+5;

vector<int>dis(N,INT_MAX);
vector<int>adj[N];

void bfs(int v){
    queue<int>q;
    q.push(v);
    dis[v]=0;
    while(!q.empty()){
        int node= q.front();
        for(int u:adj[node]){
            if(dis[node]+1<dis[u]){
                dis[u]=dis[node]+1;
                q.push(u);
            }
        }
    }
}
// shortest path in undirected graph with unit weights
